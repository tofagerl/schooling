{
"extends": "eslint:recommended",
  "plugins": [
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "__DEV__": true,
    "__SERVER__": true,
    "jQuery": true,
    "$": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures":{
      "jsx": true,
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "modules": true,
      "generators": true,
      "spread": true,
      "templateStrings": true,
      }
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": 0
  }
}
